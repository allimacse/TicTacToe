<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Ficha">
        <item name="android:src">@drawable/ficha</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_margin">0dp</item>
    </style>

    <style name="Jugador">
        <item name="android:src">@drawable/player</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_margin">0dp</item>
    </style>

    <style name="Android">
        <item name="android:src">@drawable/android</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_margin">0dp</item>
    </style>

    <style name="Theme.AppTheme.Light.NoActionBar.Food" parent="Theme.AppCompat.Light.NoActionBar"></style>
</resources>
